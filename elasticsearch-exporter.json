{
  "volumes": null,
  "id": "/exporters/elasticsearch-exporter",
  "cmd": null,
  "args": null,
  "user": null,
  "env": {
    "VAULT_HOST": "vault.service.{{json_consul_domain}}",
    "LOG_LEVEL_EXPORTER": "info",
    "LOG_LEVEL": "info",
    "VAULT_PORT": "8200",
    "ELASTIC_HOST": "https://coordinator.elasticsearchstratio.l4lb.thisdcos.directory:9200",
    "APPROLE": {
      "secret": "role"
    }
  },
  "instances": 1,
  "cpus": 0.05,
  "mem": 128,
  "disk": 0,
  "gpus": 0,
  "executor": null,
  "constraints": null,
  "fetch": null,
  "storeUrls": null,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "docker": {
      "image": "{{docker_registry}}/elasticsearch-exporter:{{ hostvars['localhost'].elasticsearch_exporter_version }}",
      "forcePullImage": false,
      "privileged": false,
      "portMappings": [
        {
          "containerPort": 9108,
          "protocol": "tcp",
          "name": "metrics",
          "servicePort": 0
        }
      ],
      "network": "USER"
    },
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/etc/resolv.conf",
        "hostPath": "/etc/resolv.conf",
        "mode": "RO"
      }
    ]
  },
  "healthChecks": [
    {
      "protocol": "TCP",
      "gracePeriodSeconds": 30,
      "intervalSeconds": 30,
      "timeoutSeconds": 15,
      "maxConsecutiveFailures": 3
    }
  ],
  "readinessChecks": null,
  "dependencies": null,
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "labels": null,
  "acceptedResourceRoles": null,
  "ipAddress": {
    "networkName": "stratio"
  },
  "residency": null,
  "secrets": {
    "role": {
      "source": "open"
    }
  },
  "taskKillGracePeriodSeconds": null
}